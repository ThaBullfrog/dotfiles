{
    "[html]": {
        "editor.tabSize": 2
    },
    "[ruby]": {
        "editor.tabSize": 2
    },
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.fontFamily": "'Inconsolata', monospace",
    "editor.fontSize": 16,
    "editor.minimap.enabled": false,
    "editor.rulers": [72],
    "editor.wordWrap": "on",
    "extensions.autoUpdate" : false,
    "terminal.integrated.fontFamily": "'Inconsolata', monospace",
    "terminal.integrated.fontSize": 16,
    "window.restoreWindows": "none",
    "workbench.colorCustomizations": {
        "terminal.ansiBlack": "#1E2127",
        "terminal.ansiBlue": "#61AFEF",
        "terminal.ansiBrightBlack": "#5C6370",
        "terminal.ansiBrightBlue": "#61AFEF",
        "terminal.ansiBrightCyan": "#56B6C2",
        "terminal.ansiBrightGreen": "#98C379",
        "terminal.ansiBrightMagenta": "#C678DD",
        "terminal.ansiBrightRed": "#E06C75",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.ansiBrightYellow": "#D19A66",
        "terminal.ansiCyan": "#56B6C2",
        "terminal.ansiGreen": "#98C379",
        "terminal.ansiMagenta": "#C678DD",
        "terminal.ansiRed": "#E06C75",
        "terminal.ansiWhite": "#ABB2BF",
        "terminal.ansiYellow": "#D19A66",
        "terminal.foreground": "#ABB2BF",
        "terminal.selectionBackground": "#3A3F4B",
        "terminalCursor.background": "#5C6370"
    },
    "workbench.colorTheme": "One Dark Pro",
    "workbench.panel.defaultLocation": "right",
    "workbench.startupEditor": "newUntitledFile"
}
